<script type"text/javascript">
    

    function setCheckMed(text) {
        switch (text) {
            case 'Nada':
                $('input[name="Medicamentos"]').eq(0).attr('checked', 'checked');
                break;
            case 'Algo':
                $('input[name="Medicamentos"]').eq(1).attr('checked', 'checked');
                break;
            case 'Bastante':
                $('input[name="Medicamentos"]').eq(2).attr('checked', 'checked');
                break;
            case 'Mucho':
                $('input[name="Medicamentos"]').eq(3).attr('checked', 'checked');
                break;
        }
    }
    function setCheck_cir_bar(text) {
        switch (text) {
            case 'Nada':
                $('input[name="CirugiaBariatrica"]').eq(0).attr('checked', 'checked');
                break;
            case 'Algo':
                $('input[name="CirugiaBariatrica"]').eq(1).attr('checked', 'checked');
                break;
            case 'Bastante':
                $('input[name="CirugiaBariatrica"]').eq(2).attr('checked', 'checked');
                break;
            case 'Mucho':
                $('input[name="CirugiaBariatrica"]').eq(3).attr('checked', 'checked');
                break;
        }
    }

    function setCheck_cir_plas(text) {
        switch (text) {
            case 'Nada':
                $('input[name="CirugiaPlastica"]').eq(0).attr('checked', 'checked');
                break;
            case 'Algo':
                $('input[name="CirugiaPlastica"]').eq(1).attr('checked', 'checked');
                break;
            case 'Bastante':
                $('input[name="CirugiaPlastica"]').eq(2).attr('checked', 'checked');
                break;
            case 'Mucho':
                $('input[name="CirugiaPlastica"]').eq(3).attr('checked', 'checked');
                break;
        }
    }

    function setCheck_hierbas(text) {
        switch (text) {
            case 'Nada':
                $('input[name="Hierbas"]').eq(0).attr('checked', 'checked');
                break;
            case 'Algo':
                $('input[name="Hierbas"]').eq(1).attr('checked', 'checked');
                break;
            case 'Bastante':
                $('input[name="Hierbas"]').eq(2).attr('checked', 'checked');
                break;
            case 'Mucho':
                $('input[name="Hierbas"]').eq(3).attr('checked', 'checked');
                break;
        }
    }
    function setCheckOtros(text) {
        switch (text) {
            case 'Nada':
                $('input[name="Otros"]').eq(0).attr('checked', 'checked');
                break;
            case 'Algo':
                $('input[name="Otros"]').eq(1).attr('checked', 'checked');
                break;
            case 'Bastante':
                $('input[name="Otros"]').eq(2).attr('checked', 'checked');
                break;
            case 'Mucho':
                $('input[name="Otros"]').eq(3).attr('checked', 'checked');
                break;
        }
    }

    function setDrop(text, grupo) {
        switch (grupo) {
            case 'medicamentos':
                $('#medicamentos').text(text);
                setCheckMed(text);
                break;
            case 'cirugia bariatrica':
                $('#cirugia_bariatrica').text(text);
                setCheck_cir_bar(text);
                break;
            case 'cirugia plastica':
                $('#cirugia_plastica').text(text);
                setCheck_cir_plas(text);
                break;
            case 'hierbas':
                $('#hierbas').text(text);
                setCheck_hierbas(text);
                break;
            case 'otros':
                $('#otros').text(text);
                setCheckOtros(text);
                break;
        }
    }

    function subirDatos() {
        setearCheckboxes();
        postForm("SaveRelacionTratamientoMotivacion");
    }

    function guardarDatos(){
        setearCheckboxes();
        postFormGuardar("SaveRelacionTratamientoMotivacion");
    }

    $(document).ready(function () {
        $('a[role="menuitem"]').click(function () {
            setDrop($(this).text(),$(this).attr('group'));
        });
        setInterval(subirDatos, 5000);
    });
</script>

