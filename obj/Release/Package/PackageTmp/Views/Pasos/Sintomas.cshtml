<script type="text/javascript">
    function subirDatos() {
        postForm("SaveSintomas");
    }

    function guardarDatos() {
        postFormGuardar("SaveSintomas");
    }

    $(document).ready(function () {
        setInterval(subirDatos, 5000);
    });
</script>

